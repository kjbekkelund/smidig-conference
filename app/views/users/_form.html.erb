<% form_for @user do |f| %>
  <%= f.error_messages %>
  <!--<p>
    <%# f.label :openid_identifier %><br />
    <%# f.text_field :openid_identifier %>
  </p>-->
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>                                                   
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </p>
  <p>
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </p>
  <p>
    <%= f.label :billing_address %><br />
    <%= f.text_area :billing_address, :cols => 35, :rows => 5 %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
